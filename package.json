    "format": "prettier --write ."
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "mocha": "^10.2.0",
    "prettier": "^3.3.3",
    "vite": "^5.4.9",
    "vite-plugin-dts": "^4.2.4"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "axios-retry": "4.5.0"
